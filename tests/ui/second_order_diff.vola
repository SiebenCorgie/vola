module std;

operation MyAdOp();

impl MyAdOp<sub> for SDF3D(at){
    let sd = eval sub.SDF3D(at);
    let second_diff = diff(sd, at);
    let first_diff = diff(second_diff, at);
    length(first_diff.x)
}

export fn mysdf(at: vec3) -> real{
    csg sphere = MyAdOp(){Box([1.0; 3])};
    eval sphere.SDF3D(at)
}
