module std;

//Moves two cubes sideways, left and right. So we _should_ be exactly on the boundary
fn mydag() -> csg{
    csg mysphere = Sphere(1.0);
    Union(){
        Translate([1.0, 0.0, 0.0]){mysphere}
    }{
        Translate([-1.0, 0.0, 0.0]){mysphere}
    }
}

export fn myfield(at: vec3) -> real{
	csg f = mydag();
	eval f.SDF3D(at)
}
